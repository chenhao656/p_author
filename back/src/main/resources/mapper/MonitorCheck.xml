<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.heeexy.example.dao.MonitorCheckDao">

    <select id="listMonitorCheck" resultType="com.alibaba.fastjson.JSONObject">
        SELECT pc.name name,pc.sex sex,
        pc.id_card id_card FROM p_person_control pc ,p_person_info pi
        WHERE pc.id_card=pi.id_card
    </select>

</mapper>